    <string name="how_to_login">How to login?</string>
    <string name="how_to_login_answer">To log in, click the Login button from Main Menu Activity and then enter your username and then select your user type</string>
    <string name="how_to_login_guide">Guide: If you forgot password contact your admin</string>

    <string name="how_to_logout">How to logout?</string>
    <string name="how_to_logout_answer">To log out, go to profile click Logout from bottom and confirm to logout</string>
    <string name="how_to_logout_guide">Guide: Remember to log out after use.</string>

    <string name="how_to_do_transaction">How to do a transaction?</string>
    <string name="how_to_do_transaction_answer">Navigate to Transactions section on Main Menu activity and select transaction type that you want to do</string>
    <string name="how_to_do_transaction_guide">Guide: Double-check details before confirming.</string>

    <string name="how_to_reverse_transaction">How to reverse a transaction?</string>
    <string name="how_to_reverse_transaction_answer">Go to Transactions on Main Menu activity and select Reversal from transaction types and then enter your approval code then confirm to reverse the transaction</string>
    <string name="how_to_reverse_transaction_guide">Guide: Reversal only within 24 hours before settlement done.</string>

    <string name="how_to_do_settlement">How to do settlement?</string>
    <string name="how_to_do_settlement_answer">Go to Settlement and follow prompts</string>
    <string name="how_to_do_settlement_guide">Guide: Only accessible after supervisor approval.</string>

